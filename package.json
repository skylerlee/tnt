{
  "name": "tnt",
  "version": "1.0.0",
  "repository": "git@github.com:skylerlee/tnt.git",
  "author": "skylerlee <skyler.ac.lee@gmail.com>",
  "license": "MIT",
  "private": true,
  "targets": {
    "main": {
      "context": "electron-main",
      "source": "src/main/index.ts"
    },
    "preload": {
      "context": "electron-main",
      "source": "src/preload/index.ts"
    },
    "renderer": {
      "context": "electron-renderer",
      "includeNodeModules": true,
      "source": "src/renderer/index.html",
      "distDir": "dist/renderer",
      "publicUrl": "."
    }
  },
  "scripts": {
    "build": "vite build",
    "build:asar": "asar pack dist out/app.asar",
    "rebuild": "electron-rebuild",
    "start": "electron dist"
  },
  "dependencies": {
    "@vscode/codicons": "^0.0.35",
    "@vscode/windows-process-tree": "^0.6.0",
    "@xterm/addon-fit": "^0.10.0",
    "@xterm/addon-webgl": "^0.18.0",
    "@xterm/xterm": "^5.5.0",
    "node-pty": "^1.0.0",
    "radash": "^12.1.0",
    "rxjs": "^7.8.1",
    "solid-js": "^1.8.16"
  },
  "devDependencies": {
    "@babel/core": "^7.12.0",
    "@biomejs/biome": "^1.6.3",
    "@electron/asar": "^3.2.9",
    "@electron/rebuild": "^3.6.0",
    "@unocss/postcss": "^0.59.0",
    "electron": "^25.9.8",
    "postcss": "^8.4.21",
    "sass": "^1.77.2",
    "unocss": "^0.59.0",
    "vite": "^5.2.11",
    "vite-plugin-electron": "^0.28.7",
    "vite-plugin-solid": "^2.10.2"
  }
}
