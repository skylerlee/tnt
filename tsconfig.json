{
  "compilerOptions": {
    "paths": {
      "@common/*": ["./src/common/*"]
    },
    "jsx": "preserve",
    "jsxImportSource": "solid-js",
    "noImplicitAny": true,
    "noImplicitOverride": true,
    "noImplicitReturns": true,
    "noImplicitThis": true
  },
  "include": ["src/**/*", "__tests__/**/*"]
}
